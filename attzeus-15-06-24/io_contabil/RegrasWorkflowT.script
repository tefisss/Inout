                int memoria = 1000000000;
                int alvo = 1000000000;
                boolean f = false;
                for(int par = 0; par < resultados.size(); par++){
                    ttM = (ttMemoria)resultados.get(par);
                    if( ttM.memoria <= memoria){
                        alvo = par;
                        f = memoria == ttM.memoria ? true : false;
